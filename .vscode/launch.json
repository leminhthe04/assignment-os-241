{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug with Makefile",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/os",
        "args": ["os_0_mlq_paging"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "Build with Makefile"
      }
    ]
  }
  